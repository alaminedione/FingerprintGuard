{
    "manifest_version": 3,
    "name": "FingerprintGuard",
    "version": "2.0.0",
    "description": "protege la vie priv√©e des utilisateurs en usurpant les informations du navigateur. (gratuite et open source)",
    "permissions": [
        "storage",
        "scripting",
        "declarativeNetRequest",
        "contentSettings",
        "webNavigation",
        "tabs"
    ],
    "host_permissions": [
        "<all_urls>"
    ],
    "action": {
        "default_popup": "popup.html",
        "default_icon": {
            "16": "icons/icon16.png",
            "48": "icons/icon48.png",
            "128": "icons/icon128.png"
        }
    },
    "options_ui": {
        "page": "settings.html",
        "open_in_tab": true
    },
    "background": {
        "service_worker": "background.js"
    },
    "icons": {
        "16": "icons/icon16.png",
        "48": "icons/icon48.png",
        "128": "icons/icon128.png"
    }
}
