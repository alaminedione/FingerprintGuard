{
    "manifest_version": 3,
    "name": "FingerprintGuard",
    "version": "1.0.0",
    "description": "Protection contre le fingerprinting des navigateurs",
    "permissions": [
        "storage",
        "scripting",
        "declarativeNetRequest",
        "declarativeNetRequestWithHostAccess",
        "contentSettings",
        "webNavigation",
        "tabs"
    ],
    "host_permissions": [
        "<all_urls>"
    ],
    "action": {
        "default_popup": "popup.html",
        "default_icon": {
            "16": "icons/icon16.png",
            "48": "icons/icon48.png",
            "128": "icons/icon128.png"
        }
    },
    "options_ui": {
        "page": "settings.html",
        "open_in_tab": true
    },
    "background": {
        "service_worker": "background.js"
    },
